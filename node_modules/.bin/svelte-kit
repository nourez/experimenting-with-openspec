#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nourezrawji/Developer/experimenting-with-openspec/node_modules/.pnpm/@sveltejs+kit@2.48.5_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.43.10_vite@5.4.21_@typ_f63d8e6424580c6ba7f9ca42bb60d3d2/node_modules/@sveltejs/kit/node_modules:/Users/nourezrawji/Developer/experimenting-with-openspec/node_modules/.pnpm/@sveltejs+kit@2.48.5_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.43.10_vite@5.4.21_@typ_f63d8e6424580c6ba7f9ca42bb60d3d2/node_modules/@sveltejs/node_modules:/Users/nourezrawji/Developer/experimenting-with-openspec/node_modules/.pnpm/@sveltejs+kit@2.48.5_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.43.10_vite@5.4.21_@typ_f63d8e6424580c6ba7f9ca42bb60d3d2/node_modules:/Users/nourezrawji/Developer/experimenting-with-openspec/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nourezrawji/Developer/experimenting-with-openspec/node_modules/.pnpm/@sveltejs+kit@2.48.5_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.43.10_vite@5.4.21_@typ_f63d8e6424580c6ba7f9ca42bb60d3d2/node_modules/@sveltejs/kit/node_modules:/Users/nourezrawji/Developer/experimenting-with-openspec/node_modules/.pnpm/@sveltejs+kit@2.48.5_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.43.10_vite@5.4.21_@typ_f63d8e6424580c6ba7f9ca42bb60d3d2/node_modules/@sveltejs/node_modules:/Users/nourezrawji/Developer/experimenting-with-openspec/node_modules/.pnpm/@sveltejs+kit@2.48.5_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.43.10_vite@5.4.21_@typ_f63d8e6424580c6ba7f9ca42bb60d3d2/node_modules:/Users/nourezrawji/Developer/experimenting-with-openspec/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
